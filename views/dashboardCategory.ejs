<div class="container my-5 py-5">
  <a href="#"><button type="button" class="btn btn-outline-dark">+ Add a category</button></a>
  <div class="row">
    <% cats.forEach( (e) => { %>
    <div class="card my-3 mx-auto post-cards border-0 bg-transparent">
      <div class="row g-0 p-2 box-part">
        <div class="col-md-8 mx-auto">
          <div class="card-body">
            <h5 class="card-title"><%= e.name %></h5>
          </div>
        </div>
      </div>
    </div>

    <% }) %>
  </div>
</div>
<script>
  const head = document.querySelector('head');
  const link = document.createElement('link');
  link.setAttribute('rel', 'stylesheet');
  link.setAttribute('href', '../public/css/feeds_style.css');
  head.appendChild(link);
</script>
