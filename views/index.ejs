<section id="jumbotron" class="my-5">
  <div class="container">
    <br /><br />
    <div class="row">
      <div class="col-md-2"></div>
      <div class="col-md-4">
        <img src="../public/img/landing_page.jpg" alt="landing_page" width="300px" />
      </div>
      <div class="col-md-4 my-5">
        <br /><br /><br /><br />
        <div class="landing-head">
          <h1 class="display-4"><strong>Daniel D. Kamasi</strong></h1>
        </div>
        <div class="landing-desc">
          <p class="fs-3">Student | Software Developer | Digital Artist</p>
        </div>
        <br />
        <div class="jumbotron-button">
          <a href="/about" class="btn btn-outline-dark porto-button" role="button" aria-pressed="true">My Portfolio</a>
        </div>
      </div>
      <div class="col-md-2"></div>
    </div>
  </div>
</section>
<section id="feeds">
  <div class="container py-5">
    <div class="section-part">
      <h1 class="display-6">
        <svg xmlns="http://www.w3.org/2000/svg" width="40" height="40" fill="currentColor" class="bi bi-arrow-right" viewBox="0 0 16 16">
          <path fill-rule="evenodd" d="M1 8a.5.5 0 0 1 .5-.5h11.793l-3.147-3.146a.5.5 0 0 1 .708-.708l4 4a.5.5 0 0 1 0 .708l-4 4a.5.5 0 0 1-.708-.708L13.293 8.5H1.5A.5.5 0 0 1 1 8z" />
        </svg>
        Latest Feeds
      </h1>
    </div>
    <br />
    <div class="row latest-feeds">
      <% post.forEach((e) => { %>
        <div class="col-sm-6">
          <div class="card">
            <div class="card-body">
              <h5 class="card-title"><%= e.title %></h5>
              <p><small><%= e.dateCreated.toLocaleDateString() %></small></p>
              <p class="card-text"><%= e.desc %></p>
              <a href="/feeds/<%= e.slug %>" class="btn btn-outline-dark">Read more</a>
            </div>
          </div>
        </div>
        <% }); %>
    </div>
    <br /><br />
  </div>
</section>
